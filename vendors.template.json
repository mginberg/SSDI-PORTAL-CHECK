{
  "SMITHLAW": {
    "method": "GET",
    "url": "https://api.smithlaw.com/leads/status",
    "id_param": "leadId",
    "auth_header": {
      "Authorization": "Bearer YOUR_API_KEY"
    },
    "extra_headers": {
      "Accept": "application/json"
    },
    "status_field": "status",
    "substatus_field": "subStatus",
    "status_time_field": "statusTime",
    "canonical_id_field": "canonicalLeadId",
    "status_map": {
      "SIGNED": "SIGNED",
      "RETAINED": "SIGNED",
      "APPOINTMENT SET": "APPT_SET",
      "QUALIFIED": "QUALIFIED",
      "DUPLICATE": "DUPLICATE",
      "NOT QUALIFIED": "NQ",
      "NO ANSWER": "NO_ANSWER",
      "BAD NUMBER": "BAD_NUMBER",
      "IN PROGRESS": "OPEN",
      "NEW": "OPEN"
    },
    "billable_statuses": [
      "SIGNED",
      "QUALIFIED",
      "APPT_SET"
    ]
  }
}